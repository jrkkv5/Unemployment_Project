
# Install censusapi package from rcran
install.packages("censusapi")
# Load censusapi package
library(censusapi)

# Add key to .Renviron
Sys.setenv(CENSUS_KEY= 1c255c9c3fb15e6f197dd34a721d696d0bd28539)
# Reload .Renviron
readRenviron("~/.Renviron")
# Check to see that the expected key is output in your R console
Sys.getenv("CENSUS_KEY")

# view list of all APIs
apis <- listCensusApis()
View(apis)


# Sample code to get census data from QWI API database
qwi <- getCensus(
  name = "timeseries/qwi/sa",
  region = "state:02",
  vars = c("Emp", "sex"),
  year = 2012,
  quarter = 1,
  agegrp = "A07",
  ownercode = "A05",
  firmsize = 1,
  seasonadj = "U",
  industry = 21)
qwi
